<?xml version="1.0" encoding="UTF-8"?>

<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                                      http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

      <entity name="aibianchi\ExactOnlineBundle\Entity\Exact" table="exact" repository-class="aibianchi\ExactOnlineBundle\doctrine\ORM\ExactRepository" >
    

    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>

    	 <field name="accessToken" 	column="access_token" 	type="text" 	/>
    	 <field name="refreshToken" column="refresh_token" 	type="text" 	/>
       <field name="code" column="code"  type="text" />
    	 <field name="tokenExpires" column="token_expires" 	type="integer"	/>
    	 <field name="enabled" 		column="enabled" 		type="boolean" 	/>
    	 <field name="createdAt" 	column="created_at" 	type="datetime" />

    </entity>
</doctrine-mapping>
